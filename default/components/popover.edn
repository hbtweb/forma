{:popover
 {:slots [:trigger :content]
  :props [:open :on-open-change :placement :offset]
  :a11y {:role "dialog"}
  :default-props {:open false :placement :bottom :offset 4}
  :schema-hints {:placement [:top :bottom :left :right :top-start :top-end :bottom-start :bottom-end]}
  :platform-support [:web :mobile :desktop]
  :platform-notes "Floating content positioned relative to trigger"}}

